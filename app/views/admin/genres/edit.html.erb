<p></p>


<div class ="container">
  <h3 class ="bg-light col-md-3">ジャンル編集</h3>
  <div class ="row">
   
    
 

    <div class ="col-md-8">
      <%= form_with model: @genre, url: admin_update_genres_path do |f| %>
        <%= f.label :ジャンル名, class:"mr-5 mt-5 mb-5"%>
        <%= f.text_field :name, autocomplete: "name", class:"mr-5" %>
        <%= f.submit '変更を保存', class:"btn btn-success btn-lg" %>
      <% end %>
    </div>
  </div>
</div>